{
  "type": "module",
  "name": "e-comm",
  "version": "1.0.0",
  "description": "A simple e-commerce application built with React and Vite.",
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test": "vitest",
    "knex": "knex --esm --knexfile ./server/db/knexfile.js",
    "migrate:make": "npm run knex migrate:make",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed:make": "npm run knex seed:make",
    "seed:run": "npm run knex seed:run",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [],
  "author": "Karl Young",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@tanstack/react-query": "^5.29.2",
    "@tanstack/react-query-devtools": "^5.29.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "css-loader": "^7.1.1",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "express": "^4.19.2",
    "globals": "^15.0.0",
    "jsdom": "^24.0.0",
    "knex": "^3.0.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "sqlite3": "^5.1.7",
    "superagent": "^8.1.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0",
    "vite": "^5.2.8",
    "vitest": "^1.5.0"
  }
}
